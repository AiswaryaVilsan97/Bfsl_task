  </servlet>
  <servlet>
  	<servlet-name>FetchNewsServlet</servlet-name>
  	<display-name>FetchNewsServlet</display-name>
  	<description></description>
  	<servlet-class>bfsl_Task1.FetchNewsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<url-pattern>/UserLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FetchApiServlet</servlet-name>
  	<url-pattern>/FetchApiServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FetchNewsServlet</servlet-name>
  	<url-pattern>/FetchNewsServlet</url-pattern>
  </servlet-mapping>
