  <servlet>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<display-name>UserLoginServlet</display-name>
  	<description></description>
  	<servlet-class>bfsl_Task1.UserLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FetchApiServlet</servlet-name>
  	<display-name>FetchApiServlet</display-name>
